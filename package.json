{
  "name": "WeatherApp",
  "version": "0.1.1",
  "description": "A seed project for a largescale angular architecture in webpack and typescript",
  "main": "/",
  "scripts": {
    "config": "node ./webpack/copy-config.js",
    "watch:dev": "webpack-dev-server --config webpack/webpack.dev.js",
    "watch:dev:mockups": "webpack-dev-server --config webpack/webpack.dev.mockups.js --content-base src/ --port 3100 --watch --NODE_ENV=dev",
    "server:proxy": "./node_modules/.bin/http-server node_modules -p 9090",
    "test": "karma start karma.conf.build.js",
    "watch:test": "npm run config dev src && karma start --auto-watch --no-single-run --NODE_ENV=test",
    "build": "webpack --config webpack/webpack.build.js --NODE_ENV=production",
    "server": "node ./webpack/server.js",
    "coverage": "open ./reports/coverage/index.html",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "e2e": "protractor",
    "e2e:delay": "node protractor.timeout.js 60000",
    "e2e:build": "npm-run-all -s e2e:delay e2e",
    "e2e:run": "npm-run-all -p -r webdriver:start server:proxy watch:dev:mockups e2e:build",
    "postinstall": "typings i"
  },
  "dependencies": {
